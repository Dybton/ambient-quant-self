{
  "manifest_version": 2,
  "name": "Hello Logger",
  "version": "1.0",
  "description": "Logs 'hello' to the console every time the extension icon is clicked",
  "icons": {
    "16": "icons/icon-16.png" 
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon-16.png",
    "default_title": "Log Hello"
  },
  "permissions": ["activeTab"]
}
